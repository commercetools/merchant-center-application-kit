{
  "name": "avengers-app",
  "cloudIdentifier": "gcp-eu",
  "entryPointUriPath": "avengers",
  "env": {
    "development": {
      "initialProjectKey": "test"
    },
    "production": {
      "applicationId": "app-id-123",
      "url": "https://avengers.app",
      "cdnUrl": "https://cdn.avengers.app"
    }
  },
  "additionalEnv": {
    "numberOfMovies": 4,
    "empty": ""
  },
  "oAuthScopes": {
    "view": ["view_products"],
    "manage": []
  },
  "additionalOAuthScopes": [
    {
      "name": "movies",
      "view": ["view_products"],
      "manage": []
    },
    {
      "name": "merch",
      "view": ["view_channels"],
      "manage": ["manage_channels"]
    }
  ],
  "headers": {
    "csp": {
      "script-src": ["https://track.avengers.app"],
      "connect-src": ["https://track.avengers.app"]
    },
    "permissionsPolicies": {
      "microphone": "()"
    },
    "strictTransportSecurity": ["includeSubDomains"]
  },
  "icon": "<svg><path fill=\"#000000\" /></svg>",
  "mainMenuLink": {
    "defaultLabel": "Avengers",
    "labelAllLocales": [],
    "permissions": []
  },
  "submenuLinks": []
}
