{
  "name": "@commercetools-frontend/application-components",
  "version": "15.2.1",
  "description": "Generic components for building Merchant Center applications",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/application-components.cjs.js",
  "module": "./dist/application-components.es.js",
  "typings": "./dist/typings/index.d.ts",
  "types": "./dist/typings/index.d.ts",
  "files": [
    "dist",
    "materials/media-queries.css",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "./../../scripts/version.js replace",
    "prebuild": "rimraf dist/**",
    "build": "yarn build:bundles && yarn build:typings",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js -i ./src/index.ts",
    "build:bundles:watch": "yarn build:bundles -w",
    "build:typings": "cross-env tsc -p tsconfig.declarations.json --emitDeclarationOnly --declarationDir dist/typings"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.6.3",
    "@commercetools-frontend/assets": "15.0.0",
    "@commercetools-frontend/constants": "15.1.4",
    "@emotion/core": "10.0.22",
    "@emotion/styled": "10.0.22",
    "lodash": "4.17.15",
    "lodash-es": "4.17.15",
    "prop-types": "15.7.2",
    "react-modal": "3.10.1",
    "react-required-if": "1.0.3",
    "warning": "4.0.3"
  },
  "devDependencies": {
    "@commercetools-frontend/ui-kit": "10.4.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-intl": "3.4.0"
  },
  "peerDependencies": {
    "@commercetools-frontend/ui-kit": "10.x",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-intl": "3.x"
  }
}
