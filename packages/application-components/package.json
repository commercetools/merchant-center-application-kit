{
  "name": "@commercetools-frontend/application-components",
  "version": "11.1.0",
  "description": "Generic components for building Merchant Center applications",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/application-components.cjs.js",
  "module": "dist/application-components.es.js",
  "files": [
    "dist",
    "materials/media-queries.css",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f cjs ./index.js -o dist/application-components.cjs.js",
    "build:es": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f es ./index.js -o dist/application-components.es.js",
    "build:es:watch": "npm run build:es -- -w",
    "storybook:build": "build-storybook -o .public -s .storybook/assets",
    "storybook:start": "start-storybook -p 9002 -s .storybook/assets"
  },
  "dependencies": {
    "@commercetools-frontend/constants": "7.0.0",
    "prop-types": "15.7.2",
    "react-modal": "3.8.1",
    "warning": "4.0.3"
  },
  "devDependencies": {
    "@commercetools-frontend/assets": "11.0.0",
    "@commercetools-frontend/ui-kit": "9.0.0",
    "@storybook/addon-actions": "4.1.14",
    "@storybook/addon-knobs": "4.1.14",
    "@storybook/addon-links": "4.1.14",
    "@storybook/addon-options": "4.1.14",
    "@storybook/addon-storysource": "4.1.14",
    "@storybook/react": "4.1.14",
    "html-loader": "0.5.5",
    "markdown-loader": "5.0.0",
    "moment-locales-webpack-plugin": "1.0.7",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-intl": "2.8.0",
    "react-testing-library": "6.0.0",
    "storybook-readme": "4.0.5"
  },
  "peerDependencies": {
    "@commercetools-frontend/assets": "9.x",
    "@commercetools-frontend/ui-kit": "9.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-intl": "2.x",
    "react-testing-library": "5.x || 6.x"
  }
}
