{
  "name": "@commercetools-frontend/actions-global",
  "version": "14.4.0",
  "description": "Global redux actions for a MC application",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/actions-global.cjs.js",
  "module": "./dist/actions-global.es.js",
  "typings": "./dist/typings/index.d.ts",
  "types": "./dist/typings/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "./../../scripts/version.js replace",
    "prebuild": "rimraf dist/**",
    "build": "yarn build:es && yarn build:cjs && yarn build:typings",
    "build:cjs": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f cjs ./src/index.ts -o ./dist/actions-global.cjs.js",
    "build:es": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f es ./src/index.ts -o ./dist/actions-global.es.js",
    "build:es:watch": "yarn build:es -w",
    "build:typings": "cross-env tsc -p tsconfig.declarations.json --emitDeclarationOnly --declarationDir dist/typings"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.5.5",
    "@commercetools-frontend/browser-history": "14.3.0",
    "@commercetools-frontend/constants": "14.4.0",
    "@commercetools-frontend/notifications": "14.3.0",
    "lodash": "4.17.15",
    "lodash-es": "4.17.15"
  },
  "devDependencies": {
    "react": "16.8.6",
    "react-redux": "7.1.0",
    "redux": "4.0.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-redux": "7.x",
    "redux": "4.x"
  }
}
