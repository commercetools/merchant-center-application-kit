    "@commercetools-frontend/i18n": "13.9.0",
    "@commercetools-frontend/l10n": "13.9.0",
    "@commercetools-frontend/notifications": "13.9.0",
    "@commercetools-frontend/permissions": "13.9.0",
    "@commercetools-frontend/react-notifications": "13.9.0",
    "@commercetools-frontend/sdk": "13.9.0",
    "@commercetools-frontend/sentry": "13.9.0",
    "@commercetools-frontend/storage": "13.9.0",
    "@commercetools-frontend/url-utils": "13.9.0",
    "@flopflip/launchdarkly-adapter": "2.7.0",
    "@flopflip/memory-adapter": "1.3.0",
    "@flopflip/react-broadcast": "8.3.0",
    "@sentry/browser": "5.4.0",
