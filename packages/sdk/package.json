{
  "name": "@commercetools-frontend/sdk",
  "version": "5.0.1",
  "description": "Tools for declarative fetching",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/sdk.cjs.js",
  "module": "./dist/sdk.es.js",
  "files": [
    "dist",
    "test-utils",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist/** test-utils/**",
    "build": "npm run build:es && npm run build:cjs && npm run build:test-utils:cjs",
    "build:cjs": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f cjs ./index.js -o ./dist/sdk.cjs.js",
    "build:es": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f es ./index.js -o ./dist/sdk.es.js",
    "build:es:watch": "npm run build:es -- -w",
    "build:test-utils:cjs": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f cjs ./src/test-utils/index.js --o ./test-utils/index.js"
  },
  "dependencies": {
    "@commercetools-frontend/constants": "5.0.1",
    "@commercetools/api-request-builder": "4.1.1",
    "@commercetools/sdk-client": "1.5.10",
    "@commercetools/sdk-middleware-correlation-id": "1.0.1",
    "@commercetools/sdk-middleware-http": "4.0.10",
    "@commercetools/sdk-middleware-user-agent": "1.2.11",
    "fast-equals": "1.6.1",
    "prop-types": "15.6.2",
    "query-string": "5.1.0",
    "unfetch": "3.1.1"
  },
  "devDependencies": {
    "react": "16.7.0",
    "react-redux": "6.0.0"
  },
  "peerDependencies": {
    "react": ">=16 <17",
    "react-redux": ">=6 <7"
  }
}
