{
  "name": "@commercetools-frontend/sdk",
  "version": "1.0.0-rc.3",
  "description": "Tools for declarative fetching",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/sdk.cjs.js",
  "module": "./dist/sdk.es.js",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f cjs ./index.js -o ./dist/sdk.cjs.js",
    "build:es": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f es ./index.js -o ./dist/sdk.es.js",
    "build:es:watch": "npm run build:es -- -w"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "1.0.0-rc.3",
    "@commercetools-frontend/constants": "1.0.0-rc.3",
    "@commercetools/api-request-builder": "4.1.0",
    "@commercetools/sdk-client": "1.5.9",
    "@commercetools/sdk-middleware-correlation-id": "1.0.1",
    "@commercetools/sdk-middleware-http": "4.0.6",
    "@commercetools/sdk-middleware-user-agent": "1.2.10",
    "fast-equals": "1.6.1",
    "prop-types": "15.6.2",
    "query-string": "5.1.0",
    "react-redux": "5.1.0",
    "unfetch": "3.1.1"
  },
  "devDependencies": {
    "react": "16.6.0"
  },
  "peerDependencies": {
    "react": ">=16 <17"
  }
}
