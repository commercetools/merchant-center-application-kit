{
  "name": "@commercetools-frontend/mc-scripts",
  "version": "17.0.1",
  "description": "Configuration and scripts for developing a MC application",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git",
    "directory": "packages/mc-scripts"
  },
  "homepage": "https://docs.commercetools.com/custom-applications",
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.js",
  "module": "./build/index.js",
  "files": [
    "bin",
    "build",
    "config",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "mc-scripts": "./bin/mc-scripts.js"
  },
  "scripts": {
    "prebuild": "rimraf build/**",
    "build": "babel src --out-dir build",
    "build:bundles:watch": "yarn build -w"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not op_mini all",
      "not dead"
    ],
    "development": [
      "last 2 firefox versions",
      "last 2 chrome versions"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.12.1",
    "@babel/runtime-corejs3": "7.12.1",
    "@commercetools-frontend/application-config": "17.0.1",
    "@commercetools-frontend/assets": "16.15.3",
    "@commercetools-frontend/babel-preset-mc-app": "17.0.1",
    "@commercetools-frontend/mc-dev-authentication": "17.0.0",
    "@commercetools-frontend/mc-html-template": "17.0.1",
    "@svgr/webpack": "5.4.0",
    "babel-loader": "8.1.0",
    "browserslist": "4.14.5",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.6.5",
    "css-loader": "4.3.0",
    "cssnano": "4.1.10",
    "file-loader": "6.1.1",
    "fs-extra": "9.0.1",
    "graphql-tag": "^2.11.0",
    "html-webpack-plugin": "4.5.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "1.2.0",
    "moment-locales-webpack-plugin": "1.2.0",
    "mri": "1.1.6",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss": "8.1.4",
    "postcss-color-mod-function": "3.0.3",
    "postcss-custom-media": "7.0.8",
    "postcss-custom-properties": "10.0.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "4.0.4",
    "postcss-preset-env": "6.7.0",
    "postcss-reporter": "6.0.1",
    "postcss-safe-parser": "5.0.2",
    "react-dev-utils": "10.2.1",
    "serve-handler": "6.1.3",
    "shelljs": "0.8.4",
    "style-loader": "1.3.0",
    "svg-url-loader": "6.0.0",
    "terser-webpack-plugin": "4.2.3",
    "thread-loader": "2.1.3",
    "url-loader": "4.1.1",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-dev-server": "3.11.0",
    "webpackbar": "4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          },
          "modules": "commonjs",
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 3
        }
      ]
    ]
  }
}
