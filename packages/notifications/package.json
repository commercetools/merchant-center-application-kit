{
  "name": "@commercetools-frontend/notifications",
  "version": "13.12.2",
  "description": "A general-purpose notification system built on top of redux",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/notifications.cjs.js",
  "module": "./dist/notifications.es.js",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "./../../scripts/version.js replace",
    "prebuild": "rimraf dist/**",
    "build": "yarn build:es && yarn build:cjs",
    "build:cjs": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f cjs ./src/index.js -o ./dist/notifications.cjs.js",
    "build:es": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f es ./src/index.js -o ./dist/notifications.es.js",
    "build:es:watch": "yarn build:es -w"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.4.5",
    "common-tags": "1.8.0"
  }
}
