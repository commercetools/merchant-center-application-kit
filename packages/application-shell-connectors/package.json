{
  "name": "@commercetools-frontend/application-shell-connectors",
  "version": "1.0.0-rc.3",
  "description": "Contains complementary tools for @commercetools-frontend/application-shell",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/application-shell-connectors.cjs.js",
  "module": "dist/application-shell-connectors.es.js",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f cjs ./index.js -o dist/application-shell-connectors.cjs.js",
    "build:es": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f es ./index.js -o dist/application-shell-connectors.es.js",
    "build:es:watch": "npm run build:es -- -w"
  },
  "dependencies": {
    "@commercetools-frontend/constants": "1.0.0-rc.3",
    "lodash.get": "4.4.2",
    "lodash.omit": "4.5.0",
    "moment-timezone": "0.5.23",
    "prop-types": "15.6.2",
    "recompose": "0.26.0"
  },
  "devDependencies": {
    "react": "16.6.1",
    "react-apollo": "2.2.4"
  },
  "peerDependencies": {
    "react": ">=16 <17",
    "react-apollo": ">=2 <3"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  }
}
