  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3"
  },
  "peerDependencies": {
    "eslint": "5.x || 6.x"
  }
}
