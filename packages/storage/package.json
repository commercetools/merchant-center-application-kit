{
  "name": "@commercetools-frontend/storage",
  "version": "13.9.0",
  "description": "Getters and setters for window.localStorage",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/storage.cjs.js",
  "module": "./dist/storage.es.js",
  "typings": "./dist/typings/index.d.ts",
  "types": "./dist/typings/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "./../../scripts/version.js replace",
    "prebuild": "rimraf dist/**",
    "build": "yarn build:es && yarn build:cjs && yarn build:typings",
    "build:typings": "cross-env tsc -p tsconfig.declarations.json --emitDeclarationOnly --declarationDir ./dist/typings",
    "build:cjs": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f cjs ./src/index.ts -o ./dist/storage.cjs.js",
    "build:es": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f es ./src/index.ts -o ./dist/storage.es.js",
    "build:es:watch": "yarn build:es -w"
  }
}
