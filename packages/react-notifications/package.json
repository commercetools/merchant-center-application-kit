{
  "name": "@commercetools-frontend/react-notifications",
  "version": "14.3.2",
  "description": "React bindings for @commercetools-frontend/notifications",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/react-notifications.cjs.js",
  "module": "./dist/react-notifications.es.js",
  "typings": "./dist/typings/index.d.ts",
  "types": "./dist/typings/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "./../../scripts/version.js replace",
    "prebuild": "rimraf dist/**",
    "build": "yarn build:es && yarn build:cjs && yarn build:typings",
    "build:cjs": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f cjs ./src/index.ts -o ./dist/react-notifications.cjs.js",
    "build:es": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f es ./src/index.ts -o ./dist/react-notifications.es.js",
    "build:es:watch": "yarn build:es -w",
    "build:typings": "cross-env tsc -p tsconfig.declarations.json --emitDeclarationOnly --declarationDir dist/typings"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.5.5",
    "@commercetools-frontend/actions-global": "14.3.0",
    "@commercetools-frontend/constants": "14.3.0",
    "@commercetools-frontend/notifications": "14.3.0",
    "@commercetools-frontend/sentry": "14.3.0",
    "@emotion/core": "10.0.14",
    "@emotion/styled": "10.0.14",
    "lodash": "4.17.15",
    "lodash-es": "4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "prop-types": "15.7.2",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@commercetools-frontend/ui-kit": "10.1.2",
    "@testing-library/react": "8.0.7",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-intl": "3.1.1",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1"
  },
  "peerDependencies": {
    "@commercetools-frontend/ui-kit": "10.x",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-intl": "3.x",
    "react-redux": "7.x",
    "react-router": "5.x",
    "react-router-dom": "5.x"
  }
}
