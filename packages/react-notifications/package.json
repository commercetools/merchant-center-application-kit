{
  "name": "@commercetools-frontend/react-notifications",
  "version": "4.4.0",
  "description": "React bindings for @commercetools-frontend/notifications",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/react-notifications.cjs.js",
  "module": "./dist/react-notifications.es.js",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f cjs ./index.js -o ./dist/react-notifications.cjs.js",
    "build:es": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js  -f es ./index.js -o ./dist/react-notifications.es.js",
    "build:es:watch": "npm run build:es -- -w"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "4.4.0",
    "@commercetools-frontend/constants": "4.3.0",
    "@commercetools-frontend/notifications": "4.0.0",
    "@commercetools-frontend/sentry": "4.0.0",
    "classnames": "2.2.6",
    "lodash": "4.17.11",
    "lodash-es": "4.17.11",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "prop-types": "15.6.2",
    "recompose": "^0.26.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@commercetools-frontend/ui-kit": "5.0.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-intl": "2.7.2",
    "react-redux": "6.0.0",
    "react-router": "4.4.0-beta.6",
    "react-router-dom": "4.4.0-beta.6"
  },
  "peerDependencies": {
    "@commercetools-frontend/ui-kit": ">=5 <6",
    "react": ">=16 <17",
    "react-dom": ">=16 <17",
    "react-intl": ">=2 <3",
    "react-redux": ">=6 <7",
    "react-router": ">=4 <5 || >=4.4.0-beta.0 <5",
    "react-router-dom": ">=4 <5 || >=4.4.0-beta.0 <5"
  }
}
