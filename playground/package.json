    "@types/jest": "^29.5.4",
    "@types/node": "^22.13.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vercel/node": "^2.15.10",
    "dotenv-flow": "^4.0.0",
    "express": "4.20.0",
    "jest": "29.7.0",
    "msw": "0.49.3",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=22",
    "pnpm": ">=8"
  }
