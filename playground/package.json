{
  "name": "@commercetools-local/playground",
  "version": "24.8.0",
  "description": "A playground application to get started developing Merchant Center Applications",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start:server": "NODE_ENV=development && ts-node ./src/server.ts",
    "start": "mc-scripts start",
    "compile-html": "mc-scripts compile-html",
    "compile-html:local": "MC_APP_ENV=development mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "pnpm compile-html:local && mc-scripts serve",
    "extract-intl": "formatjs extract --format=./intl-formatter.js --out-file=./src/i18n/data/core.json 'src/**/!(*.spec).js'",
    "deploy": "vercel --scope=commercetools --confirm --no-clipboard"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.22.15",
    "@commercetools-docs/ui-kit": "24.11.0",
    "@commercetools-frontend/actions-global": "24.8.0",
    "@commercetools-frontend/application-components": "24.8.0",
    "@commercetools-frontend/application-shell": "24.8.0",
    "@commercetools-frontend/application-shell-connectors": "24.8.0",
    "@commercetools-frontend/assets": "24.8.0",
    "@commercetools-frontend/browser-history": "24.8.0",
    "@commercetools-frontend/constants": "24.8.0",
    "@commercetools-frontend/i18n": "24.8.0",
    "@commercetools-frontend/l10n": "24.8.0",
    "@commercetools-frontend/permissions": "24.8.0",
    "@commercetools-frontend/sdk": "24.8.0",
    "@commercetools-uikit/card": "^20.2.3",
    "@commercetools-uikit/checkbox-input": "^20.2.3",
    "@commercetools-uikit/constraints": "^20.2.3",
    "@commercetools-uikit/data-table": "^20.2.3",
    "@commercetools-uikit/date-time-input": "^20.2.3",
    "@commercetools-uikit/flat-button": "^20.2.3",
    "@commercetools-uikit/grid": "^20.2.3",
    "@commercetools-uikit/hooks": "^20.2.3",
    "@commercetools-uikit/icons": "^20.2.3",
    "@commercetools-uikit/label": "^20.2.3",
    "@commercetools-uikit/link": "^20.2.3",
    "@commercetools-uikit/loading-spinner": "^20.2.3",
    "@commercetools-uikit/notifications": "^20.2.3",
    "@commercetools-uikit/pagination": "^20.2.3",
    "@commercetools-uikit/primary-button": "^20.2.3",
