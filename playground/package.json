    "@commercetools-frontend/constants": "1.0.0-beta.35",
    "@commercetools-frontend/i18n": "1.0.0-beta.35",
    "@commercetools-frontend/l10n": "1.0.0-beta.35",
    "@commercetools-frontend/mc-http-server": "1.0.0-beta.35",
    "@commercetools-frontend/permissions": "1.0.0-beta.35",
    "@commercetools-frontend/sdk": "1.0.0-beta.35",
    "@commercetools-frontend/ui-kit": "2.0.0-rc.11",
    "moment": "2.22.2",
    "moment-timezone": "0.5.21",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-intl": "2.7.1",
