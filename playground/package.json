{
  "name": "playground",
  "version": "12.0.0",
  "description": "A playground application to get started developing Merchant Center Applications",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod:local": "NODE_ENV=production mc-http-server --csp=$(pwd)/csp.json --config=$(pwd)/env.json --use-local-assets",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "deploy": "npm run build; rm dist/assets/*.js.map; npm run deploy:eu; npm run deploy:us",
    "deploy:eu": "now --local-config=now.eu.json --docker; now alias --local-config=now.eu.json",
    "deploy:us": "now --local-config=now.us.json --docker; now alias --local-config=now.us.json"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "12.0.0",
    "@commercetools-frontend/application-shell": "12.0.0",
    "@commercetools-frontend/application-shell-connectors": "12.0.0",
    "@commercetools-frontend/constants": "12.0.0",
    "@commercetools-frontend/i18n": "12.0.0",
    "@commercetools-frontend/l10n": "12.0.0",
    "@commercetools-frontend/mc-http-server": "12.0.0",
    "@commercetools-frontend/permissions": "12.0.0",
    "@commercetools-frontend/sdk": "12.0.0",
    "@commercetools-frontend/ui-kit": "9.1.0",
    "lodash": "4.17.11",
    "lodash-es": "4.17.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "normalizr": "3.3.0",
    "prop-types": "15.7.2",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "react-intl": "2.8.0",
    "react-redux": "6.0.1",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "react-select": "1.2.1",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@commercetools-frontend/mc-scripts": "12.0.0"
  }
}
