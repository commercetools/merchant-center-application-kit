{
  "name": "playground",
  "version": "18.6.1",
  "description": "A playground application to get started developing Merchant Center Applications",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "compile-html": "NODE_ENV=production mc-scripts compile-html --inline-csp",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development mc-scripts compile-html --inline-csp --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "extract-intl": "formatjs extract --format=./intl-formatter.js --out-file=./src/i18n/data/core.json 'src/**/!(*.spec).js'",
    "deploy": "vercel --scope=commercetools-playground --confirm --no-clipboard"
  },
  "dependencies": {
    "@commercetools-docs/ui-kit": "18.0.0",
    "@commercetools-frontend/actions-global": "20.12.1",
    "@commercetools-frontend/application-components": "20.12.1",
    "@commercetools-frontend/application-shell": "20.12.1",
    "@commercetools-frontend/application-shell-connectors": "20.12.1",
    "@commercetools-frontend/assets": "20.12.1",
    "@commercetools-frontend/constants": "20.12.1",
    "@commercetools-frontend/i18n": "20.12.1",
    "@commercetools-frontend/l10n": "20.12.1",
    "@commercetools-frontend/permissions": "20.12.1",
    "@commercetools-frontend/sdk": "20.12.1",
    "@commercetools-uikit/checkbox-input": "^12.2.2",
    "@commercetools-uikit/constraints": "^12.2.2",
    "@commercetools-uikit/data-table": "^12.2.2",
    "@commercetools-uikit/flat-button": "^12.2.2",
    "@commercetools-uikit/grid": "^12.0.7",
    "@commercetools-uikit/icons": "^12.2.2",
    "@commercetools-uikit/link": "^12.2.2",
    "@commercetools-uikit/loading-spinner": "^12.2.2",
    "@commercetools-uikit/notifications": "^12.2.2",
    "@commercetools-uikit/pagination": "^12.2.2",
    "@commercetools-uikit/primary-button": "^12.2.2",
    "@commercetools-uikit/spacings": "^12.2.2",
    "@commercetools-uikit/text": "^12.2.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.23.0",
    "react-router-dom": "5.3.0",
    "vercel": "23.1.2"
  },
  "devDependencies": {
    "@commercetools-backend/express": "20.12.1",
    "@commercetools-frontend/mc-dev-authentication": "20.10.6",
    "@commercetools-frontend/mc-scripts": "20.12.1",
    "@formatjs/cli": "4.5.0",
    "@vercel/node": "^1.12.1"
  }
}
