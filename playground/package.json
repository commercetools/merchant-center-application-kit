{
  "name": "playground",
  "version": "3.0.0",
  "description": "A playground application to get started developing Merchant Center Applications",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "deploy": "npm run build; rm dist/assets/*.js.map; npm run deploy:eu; npm run deploy:us",
    "deploy:eu": "now --local-config=now.eu.json --docker; now alias --local-config=now.eu.json",
    "deploy:us": "now --local-config=now.us.json --docker; now alias --local-config=now.us.json"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "3.0.0",
    "@commercetools-frontend/application-shell": "3.0.0",
    "@commercetools-frontend/application-shell-connectors": "3.0.0",
    "@commercetools-frontend/constants": "3.0.0",
    "@commercetools-frontend/i18n": "3.0.0",
    "@commercetools-frontend/l10n": "3.0.0",
    "@commercetools-frontend/mc-http-server": "3.0.0",
    "@commercetools-frontend/permissions": "3.0.0",
    "@commercetools-frontend/sdk": "3.0.0",
    "@commercetools-frontend/ui-kit": "2.0.0-rc.11",
    "moment": "2.22.2",
    "moment-timezone": "0.5.23",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-intl": "2.7.2",
    "react-redux": "5.1.1",
    "react-router": "4.4.0-beta.6",
    "react-router-dom": "4.4.0-beta.6",
    "react-select": "1.2.1"
  },
  "devDependencies": {
    "@commercetools-frontend/mc-scripts": "3.0.0"
  }
}
