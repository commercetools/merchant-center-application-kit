    "@types/jest": "^29.5.1",
    "@types/node": "^16.18.12",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vercel/node": "^2.9.4",
    "dotenv-flow": "^3.2.0",
    "express": "4.18.2",
    "jest": "29.5.0",
    "msw": "0.49.3",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
