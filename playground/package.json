    "@types/node": "^22.13.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vercel/node": "^2.15.10",
    "dotenv-flow": "^4.0.0",
    "express": "4.21.2",
    "jest": "29.7.0",
    "msw": "^1.3.5",
    "ts-node": "^10.9.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=22",
    "pnpm": ">=8"
  }
}
