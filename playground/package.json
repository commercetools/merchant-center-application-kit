{
  "name": "playground",
  "version": "14.1.1",
  "description": "A playground application to get started developing Merchant Center Applications",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "compile-html:eu": "mc-scripts compile-html --csp=$(pwd)/production-eu.csp.json --config=$(pwd)/production-eu.env.json --use-local-assets --transformer $(pwd)/config/transformer-now.js",
    "compile-html:us": "mc-scripts compile-html --csp=$(pwd)/production-us.csp.json --config=$(pwd)/production-us.env.json --use-local-assets --transformer $(pwd)/config/transformer-now.js",
    "start:prod:local": "NODE_ENV=production mc-http-server --csp=$(pwd)/csp.json --config=$(pwd)/env.json --use-local-assets",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "deploy": "yarn build && yarn deploy:eu && yarn deploy:us",
    "deploy:eu": "yarn compile-html:eu && now --local-config=production-eu.now.json --target production",
    "deploy:us": "yarn compile-html:us && now --local-config=production-us.now.json --target production"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "14.1.1",
    "@commercetools-frontend/application-components": "14.1.1",
    "@commercetools-frontend/application-shell": "14.1.1",
    "@commercetools-frontend/application-shell-connectors": "14.1.1",
    "@commercetools-frontend/assets": "14.0.0",
    "@commercetools-frontend/constants": "14.1.1",
    "@commercetools-frontend/i18n": "14.1.1",
    "@commercetools-frontend/l10n": "14.1.1",
    "@commercetools-frontend/mc-http-server": "14.0.0",
    "@commercetools-frontend/permissions": "14.1.1",
    "@commercetools-frontend/sdk": "14.1.1",
    "@commercetools-frontend/ui-kit": "10.0.1",
    "lodash": "4.17.15",
    "lodash-es": "4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "normalizr": "3.4.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-intl": "3.0.0-beta.14",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@commercetools-frontend/mc-scripts": "14.0.0"
  }
}
