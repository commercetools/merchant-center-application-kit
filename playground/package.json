{
  "name": "playground",
  "version": "4.4.0",
  "description": "A playground application to get started developing Merchant Center Applications",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "deploy": "npm run build; rm dist/assets/*.js.map; npm run deploy:eu; npm run deploy:us",
    "deploy:eu": "now --local-config=now.eu.json --docker; now alias --local-config=now.eu.json",
    "deploy:us": "now --local-config=now.us.json --docker; now alias --local-config=now.us.json"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "4.4.0",
    "@commercetools-frontend/application-shell": "4.4.0",
    "@commercetools-frontend/application-shell-connectors": "4.4.0",
    "@commercetools-frontend/constants": "4.3.0",
    "@commercetools-frontend/i18n": "4.4.0",
    "@commercetools-frontend/l10n": "4.4.0",
    "@commercetools-frontend/mc-http-server": "4.0.0",
    "@commercetools-frontend/permissions": "4.4.0",
    "@commercetools-frontend/sdk": "4.4.0",
    "@commercetools-frontend/ui-kit": "5.0.0",
    "lodash": "4.17.11",
    "lodash-es": "4.17.11",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "normalizr": "3.3.0",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-intl": "2.7.2",
    "react-redux": "6.0.0",
    "react-router": "4.4.0-beta.6",
    "react-router-dom": "4.4.0-beta.6",
    "react-select": "1.2.1",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@commercetools-frontend/mc-scripts": "4.4.0"
  }
}
