{
  "name": "merchant-center-application-template-starter",
  "version": "13.13.3",
  "description": "A starter example for the bare minimum setup to develop a Merchant Center Application",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod:local": "NODE_ENV=production mc-http-server --config=$(pwd)/env.json --use-local-assets",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "13.13.3",
    "@commercetools-frontend/application-components": "13.12.3",
    "@commercetools-frontend/application-shell": "13.13.3",
    "@commercetools-frontend/assets": "13.12.2",
    "@commercetools-frontend/i18n": "13.12.2",
    "@commercetools-frontend/mc-http-server": "13.13.1",
    "@commercetools-frontend/permissions": "13.13.3",
    "@commercetools-frontend/ui-kit": "10.0.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-apollo": "2.5.8",
    "react-dom": "16.8.6",
    "react-intl": "3.0.0-beta.14",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@commercetools-frontend/jest-preset-mc-app": "13.13.1",
    "@commercetools-frontend/mc-scripts": "13.13.1",
    "@testing-library/react": "8.0.4",
    "enzyme": "3.10.0",
    "jest": "24.8.0"
  }
}
