{
  "name": "merchant-center-application-template-starter",
  "version": "7.0.0",
  "description": "A starter example for the bare minimum setup to develop a Merchant Center Application",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "7.0.0",
    "@commercetools-frontend/application-shell": "7.0.0",
    "@commercetools-frontend/i18n": "7.0.0",
    "@commercetools-frontend/mc-http-server": "7.0.0",
    "@commercetools-frontend/permissions": "7.0.0",
    "@commercetools-frontend/ui-kit": "7.0.0",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-apollo": "2.3.3",
    "react-dom": "16.7.0",
    "react-intl": "2.8.0",
    "react-redux": "6.0.0",
    "react-router": "4.4.0-beta.6",
    "react-router-dom": "4.4.0-beta.6",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@commercetools-frontend/jest-preset-mc-app": "7.0.0",
    "@commercetools-frontend/mc-scripts": "7.0.0",
    "enzyme": "3.8.0",
    "jest": "23.6.0",
    "react-testing-library": "5.4.4"
  }
}
