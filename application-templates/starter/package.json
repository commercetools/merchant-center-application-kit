{
  "name": "@commercetools-frontend/application-template-starter",
  "version": "0.0.0",
  "description": "A starter example for the bare minimum setup to develop a Merchant Center Application",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "5.0.2",
    "@commercetools-frontend/application-shell": "5.0.3",
    "@commercetools-frontend/i18n": "5.0.2",
    "@commercetools-frontend/mc-http-server": "5.0.2",
    "@commercetools-frontend/permissions": "5.0.2",
    "@commercetools-frontend/ui-kit": "5.0.0",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-apollo": "2.3.3",
    "react-dom": "16.7.0",
    "react-intl": "2.7.2",
    "react-redux": "6.0.0",
    "react-router": "4.4.0-beta.6",
    "react-router-dom": "4.4.0-beta.6"
  },
  "devDependencies": {
    "@commercetools-frontend/jest-preset-mc-app": "5.0.2",
    "@commercetools-frontend/mc-scripts": "5.0.2",
    "enzyme": "3.8.0",
    "jest": "23.6.0",
    "react-testing-library": "5.4.2"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  }
}
