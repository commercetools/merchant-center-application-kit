{
  "name": "merchant-center-application-template-starter",
  "version": "15.1.7",
  "description": "A starter example for the bare minimum setup to develop a Merchant Center Application",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod:local": "NODE_ENV=production mc-http-server --config=$(pwd)/env.json --use-local-assets",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "15.1.7",
    "@commercetools-frontend/application-components": "15.1.7",
    "@commercetools-frontend/application-shell": "15.1.7",
    "@commercetools-frontend/assets": "15.0.0",
    "@commercetools-frontend/i18n": "15.1.7",
    "@commercetools-frontend/mc-http-server": "15.1.7",
    "@commercetools-frontend/permissions": "15.1.7",
    "@commercetools-frontend/ui-kit": "10.3.0",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-apollo": "3.1.2",
    "react-dom": "16.10.2",
    "react-intl": "3.3.2",
    "react-redux": "7.1.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4"
  },
  "devDependencies": {
    "@commercetools-frontend/jest-preset-mc-app": "15.1.7",
    "@commercetools-frontend/mc-scripts": "15.1.7",
    "@testing-library/react": "9.3.0",
    "enzyme": "3.10.0",
    "jest": "24.9.0"
  }
}
