{
  "name": "merchant-center-application-template-starter",
  "version": "11.3.0",
  "description": "A starter example for the bare minimum setup to develop a Merchant Center Application",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod:local": "NODE_ENV=production mc-http-server --config=$(pwd)/env.json --use-local-assets",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "11.2.0",
    "@commercetools-frontend/application-shell": "11.3.0",
    "@commercetools-frontend/i18n": "11.3.0",
    "@commercetools-frontend/mc-http-server": "11.2.0",
    "@commercetools-frontend/permissions": "11.2.0",
    "@commercetools-frontend/ui-kit": "9.1.0",
    "prop-types": "15.7.2",
    "react": "16.8.5",
    "react-apollo": "2.5.2",
    "react-dom": "16.8.5",
    "react-intl": "2.8.0",
    "react-redux": "6.0.1",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@commercetools-frontend/jest-preset-mc-app": "11.2.0",
    "@commercetools-frontend/mc-scripts": "11.2.0",
    "enzyme": "3.9.0",
    "jest": "24.5.0",
    "react-testing-library": "6.0.2"
  }
}
