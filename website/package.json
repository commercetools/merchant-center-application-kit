{
  "name": "@commercetools-website/custom-applications",
  "version": "18.3.10",
  "private": true,
  "scripts": {
    "clean": "gatsby clean",
    "build": "./scripts/prebuild.sh && yarn generate-icons && gatsby build --prefix-paths --verbose --log-pages",
    "start": "yarn generate-icons && gatsby develop",
    "serve": "gatsby serve --prefix-paths",
    "check-links": "start-server-and-test 'yarn serve' http://localhost:9000/custom-applications 'yarn linkinator --url-rewrite-replace=http://localhost:9000 http://localhost:9000/custom-applications'",
    "generate-icons": "svgr -d src/icons/generated -- src/icons/svg && prettier --write '**/generated/*.js'"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "18.2.3",
    "@commercetools-docs/ui-kit": "18.2.2",
    "@commercetools-uikit/card": "^13.0.2",
    "@commercetools-uikit/spacings-inline": "^13.0.2",
    "@commercetools-uikit/spacings-stack": "^13.0.2",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "gatsby": "4.6.0",
    "gatsby-cli": "4.6.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.24.3"
  },
  "devDependencies": {
    "@svgr/cli": "6.2.0",
    "linkinator": "3.0.3",
    "prettier": "2.5.1",
    "start-server-and-test": "1.14.0"
  }
}
