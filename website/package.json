{
  "name": "@commercetools-website/custom-applications",
  "version": "18.3.5",
  "private": true,
  "scripts": {
    "build": "./scripts/prebuild.sh && yarn generate-icons && gatsby build --prefix-paths && ./scripts/postbuild.sh && yarn check-links",
    "start": "yarn generate-icons && gatsby develop",
    "check-links": "yarn linkinator '*/index.html' --server-root public",
    "generate-icons": "svgr -d src/icons/generated src/icons/svg && prettier --write '**/generated/*.js'"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "17.0.9",
    "@commercetools-docs/ui-kit": "17.0.8",
    "@commercetools-uikit/card": "^12.2.2",
    "@commercetools-uikit/spacings-inline": "^12.2.2",
    "@commercetools-uikit/spacings-stack": "^12.2.2",
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "gatsby": "3.14.4",
    "gatsby-cli": "3.14.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.21.0"
  },
  "devDependencies": {
    "@svgr/cli": "5.5.0",
    "linkinator": "2.14.4"
  }
}
