{
  "name": "@commercetools-website/custom-applications",
  "version": "18.3.6",
  "private": true,
  "scripts": {
    "build": "./scripts/prebuild.sh && yarn generate-icons && gatsby build --prefix-paths && yarn check-links",
    "start": "yarn generate-icons && gatsby develop",
    "check-links": "start-server-and-test 'yarn gatsby serve --prefix-paths' http://localhost:9000/custom-applications 'yarn linkinator http://localhost:9000/custom-applications'",
    "generate-icons": "svgr -d src/icons/generated src/icons/svg && prettier --write '**/generated/*.js'"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "18.0.0",
    "@commercetools-docs/ui-kit": "18.0.0",
    "@commercetools-uikit/card": "^12.2.2",
    "@commercetools-uikit/spacings-inline": "^12.2.2",
    "@commercetools-uikit/spacings-stack": "^12.2.2",
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "gatsby": "4.2.0",
    "gatsby-cli": "4.2.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.21.0"
  },
  "devDependencies": {
    "@svgr/cli": "5.5.0",
    "linkinator": "2.14.4",
    "prettier": "2.4.1",
    "start-server-and-test": "1.14.0"
  }
}
