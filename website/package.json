{
  "name": "@commercetools-website/custom-applications",
  "version": "17.0.2",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "prebuild": "./scripts/prebuild.sh",
    "postbuild": "./scripts/postbuild.sh && yarn check-links",
    "start": "gatsby develop",
    "precheck-links": "cp -f linkchecker.index.html ./public/index.html",
    "check-links": "yarn linkinator ./public"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "9.0.0",
    "@commercetools-docs/ui-kit": "9.0.0",
    "@commercetools-uikit/card": "10.39.6",
    "@commercetools-uikit/spacings-inline": "10.39.6",
    "@commercetools-uikit/spacings-stack": "10.39.6",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "gatsby": "2.24.85",
    "gatsby-cli": "2.12.111",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "5.8.6"
  },
  "devDependencies": {
    "linkinator": "2.2.0"
  }
}
