{
  "name": "@commercetools-website/custom-applications",
  "version": "18.3.9",
  "private": true,
  "scripts": {
    "build": "./scripts/prebuild.sh && yarn generate-icons && gatsby build --prefix-paths --verbose --log-pages && yarn check-links",
    "start": "yarn generate-icons && gatsby develop",
    "check-links": "start-server-and-test 'yarn gatsby serve --prefix-paths' http://localhost:9000/custom-applications 'yarn linkinator http://localhost:9000/custom-applications'",
    "generate-icons": "svgr -d src/icons/generated -- src/icons/svg && prettier --write '**/generated/*.js'"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "18.1.0",
    "@commercetools-docs/ui-kit": "18.1.0",
    "@commercetools-uikit/card": "^12.2.9",
    "@commercetools-uikit/spacings-inline": "^12.2.9",
    "@commercetools-uikit/spacings-stack": "^12.2.9",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "gatsby": "4.5.4",
    "gatsby-cli": "4.5.2",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.24.3"
  },
  "devDependencies": {
    "@svgr/cli": "6.2.0",
    "linkinator": "2.16.2",
    "prettier": "2.5.1",
    "start-server-and-test": "1.14.0"
  }
}
