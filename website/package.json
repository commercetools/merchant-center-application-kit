{
  "name": "@commercetools-website/custom-applications",
  "version": "16.6.1",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "prebuild": "rm -rf public/custom-applications && ./scripts/get_latest_release.sh",
    "postbuild": "mv public custom-applications && mkdir public && mv custom-applications public/",
    "prestart": "./scripts/get_latest_release.sh",
    "start": "gatsby develop"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "2.5.0",
    "@commercetools-docs/ui-kit": "2.5.0",
    "@commercetools-uikit/card": "10.18.4",
    "@commercetools-uikit/spacings-inline": "10.18.4",
    "@commercetools-uikit/spacings-stack": "10.18.4",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "gatsby": "2.20.26",
    "gatsby-cli": "2.11.12",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "3.4.0"
  }
}
