{
  "name": "@commercetools-website/custom-applications",
  "version": "17.0.22",
  "private": true,
  "scripts": {
    "prebuild": "./scripts/prebuild.sh && yarn generate-icons",
    "build": "gatsby build --prefix-paths",
    "postbuild": "./scripts/postbuild.sh && yarn check-links",
    "prestart": "yarn generate-icons",
    "start": "gatsby develop",
    "check-links": "yarn linkinator '*/index.html' --server-root public",
    "generate-icons": "svgr -d src/icons/generated src/icons/svg && prettier --write '**/generated/*.js'"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "15.0.2",
    "@commercetools-docs/ui-kit": "15.0.2",
    "@commercetools-uikit/card": "^11.2.1",
    "@commercetools-uikit/spacings-inline": "^11.2.1",
    "@commercetools-uikit/spacings-stack": "^11.2.1",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "gatsby": "3.2.1",
    "gatsby-cli": "3.2.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.15.7"
  },
  "devDependencies": {
    "@svgr/cli": "5.5.0",
    "linkinator": "2.13.6"
  }
}
