{
  "name": "@commercetools-website/custom-applications",
  "version": "17.0.4",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "prebuild": "./scripts/prebuild.sh",
    "postbuild": "./scripts/postbuild.sh && yarn check-links",
    "start": "gatsby develop",
    "precheck-links": "cp -f linkchecker.index.html ./public/index.html",
    "check-links": "yarn linkinator ./public"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "10.0.1",
    "@commercetools-docs/ui-kit": "10.0.1",
    "@commercetools-uikit/card": "10.39.8",
    "@commercetools-uikit/spacings-inline": "10.39.8",
    "@commercetools-uikit/spacings-stack": "10.39.8",
    "@emotion/core": "10.1.1",
    "@emotion/styled": "10.0.27",
    "gatsby": "2.25.3",
    "gatsby-cli": "2.12.117",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "5.9.4"
  },
  "devDependencies": {
    "linkinator": "2.2.2"
  }
}
