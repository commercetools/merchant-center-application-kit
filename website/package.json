{
  "name": "@commercetools-website/custom-applications",
  "version": "16.10.5",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "prebuild": "./scripts/prebuild.sh",
    "postbuild": "./scripts/postbuild.sh && yarn check-links",
    "start": "gatsby develop",
    "precheck-links": "cp -f linkchecker.index.html ./public/index.html",
    "check-links": "yarn linkinator ./public"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "8.0.1",
    "@commercetools-docs/ui-kit": "8.0.0",
    "@commercetools-uikit/card": "10.30.2",
    "@commercetools-uikit/spacings-inline": "10.30.1",
    "@commercetools-uikit/spacings-stack": "10.30.1",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "gatsby": "2.24.67",
    "gatsby-cli": "2.12.102",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "5.8.4"
  },
  "devDependencies": {
    "linkinator": "2.1.3"
  }
}
