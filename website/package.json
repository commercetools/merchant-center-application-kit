{
  "name": "@commercetools-website/custom-applications",
  "version": "17.0.8",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "prebuild": "./scripts/prebuild.sh && yarn generate-icons",
    "postbuild": "./scripts/postbuild.sh && yarn check-links",
    "prestart": "yarn generate-icons",
    "start": "gatsby develop",
    "precheck-links": "cp -f linkchecker.index.html ./public/index.html",
    "check-links": "yarn linkinator ./public",
    "generate-icons": "svgr -d src/icons/generated src/icons/svg && prettier --write '**/generated/*.js'"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "12.0.1",
    "@commercetools-docs/ui-kit": "12.0.1",
    "@commercetools-uikit/card": "10.42.2",
    "@commercetools-uikit/spacings-inline": "10.42.2",
    "@commercetools-uikit/spacings-stack": "10.42.2",
    "@emotion/react": "11.1.2",
    "@emotion/styled": "11.0.0",
    "gatsby": "2.28.1",
    "gatsby-cli": "2.15.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "5.10.6"
  },
  "devDependencies": {
    "linkinator": "2.8.0"
  }
}
