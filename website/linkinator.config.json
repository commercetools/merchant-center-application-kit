{
  "__docs": "https://github.com/JustinBeckwith/linkinator/blob/main/src/config.ts#L3",
  "recurse": true,
  "skip": [
    "/.+/(page-data|static|icons)/(.*)",
    "/[^/]+/[^/]+.(js|json)$",
    "/.+/favicon(.*)",
    "https://support.commercetools.com",
    "https://developers.cloudflare.com",
    "https://dash.cloudflare.com",
    "/.+/api$",
    "/.+/api/",
    "/.+/custom-applications/legacy",
    "/.+/custom-applications/playground",
    "/.+/docs/login",
    "/.+/import-export",
    "/.+/merchant-center",
    "/.+/sdk",
    "/.+/tutorials"
  ],
  "verbosity": "error",
  "retry": true,
  "retryErrors": true,
  "retryErrorsCount": 3,
  "retryErrorsJitter": 500
}
