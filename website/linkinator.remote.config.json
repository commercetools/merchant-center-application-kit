{
  "__docs": "https://github.com/JustinBeckwith/linkinator/blob/main/src/config.ts#L3",
  "recurse": true,
  "skip": [
    ".(json|js|png|jpg|svg|css)(\\?(.*)+)?$",
    "aws.amazon.com",
    "/$",
    "/api",
    "/cli-overview",
    "/docs",
    "/faqs",
    "/getting-started",
    "/graphql-api",
    "/http-api",
    "/import-api",
    "/imprint",
    "/login",
    "/merchant-center",
    "/merchant-center-releases",
    "/playground",
    "/privacy",
    "/release-notes",
    "/sdk",
    "/software-development-kits",
    "/sunrise",
    "/tutorials",
    "/tutorials-overview",
    "/custom-applications/legacy",
    "https://developers.cloudflare.com"
  ],
  "verbosity": "error",
  "retry": true,
  "retryErrors": true,
  "retryErrorsCount": 3,
  "retryErrorsJitter": 500,
  "urlRewriteSearch": "https://docs.commercetools.com"
}
