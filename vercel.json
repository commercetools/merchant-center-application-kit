{
  "version": 2,
  "github": {
    "enabled": false
  },
  "rewrites": [
    {
      "source": "/custom-applications/playground/(.*)",
      "destination": "/playground/index.html"
    },
    {
      "source": "/custom-applications",
      "destination": "/"
    },
    {
      "source": "/custom-applications/(.*)",
      "destination": "/$1"
    }
  ],
  "redirects": [
    {
      "source": "/custom-applications/support-policy",
      "destination": "/custom-applications/maintenance-guidelines"
    },
    {
      "source": "/custom-applications/getting-started/what-is-a-custom-application",
      "destination": "/custom-applications/what-is-a-custom-application"
    }
  ]
}
